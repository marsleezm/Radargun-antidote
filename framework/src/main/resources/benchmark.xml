<bench-config>
   <master
         bindAddress="${127.0.0.1:master.address}"
         port="${21032:master.port}"/>
   <benchmark
         initSize="${4:Islaves}"
         maxSize="${4:slaves}"
         increment="1">
      <DestroyWrapper
            runOnAllSlaves="true"/>
      <StartCluster
            staggerSlaveStartup="true"
            delayAfterFirstSlaveStarts="5000"
            delayBetweenStartingSlaves="1000"/>
      <ClusterValidation
            passiveReplication="false"
            partialReplication="false"/>
      <TpccPopulation
            preloadedFromDB="false"
            numWarehouses="4"
            cLastMask="0"
            olIdMask="0"
            cIdMask="0"
            threadParallelLoad="true"
            batchLevel="100"
            numLoaderThreads="2"/>
      <ResetStats />
      <CacheSize
            statName="CACHE_SIZE_BEFORE_BENCH" />
      <TpccBenchmark
            statsSamplingInterval="5000"
            numOfThreads="1"
            perThreadSimulTime="60"
            arrivalRate="0"
            accessSameWarehouse="true"
            paymentWeight="45"
            orderStatusWeight="5"/>
      <CacheSize
            statName="CACHE_SIZE_AFTER_BENCH" />
      <CsvReportGeneration/>
   </benchmark>
   <products>
      <antidote>
         <config name="antidote-config.xml"/>
      </antidote>
   </products>
   <reports>
      <report name="Reports" />
   </reports>
</bench-config>
